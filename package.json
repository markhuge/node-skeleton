{
  "name": "node-skeleton",
  "description": "A quick node project skeleton",
  "author": "Mark Wilkerson <mark@segfawlt.net>",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "coffee src/app.coffee",
    "test": "mocha --recursive --compilers coffee:coffee-script -R spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markhuge/node-skeleton.git"
  },
  "config": {
    "blanket": {
      "pattern": "src",
      "data-cover-never": "node_modules",
      "loader": "./node-loaders/coffee-script"
    }
  },
  "engines": {
    "node": "~0.10.x"
  },
  "devDependencies": {
    "grunt-cli": "~0.1.12",
    "grunt-contrib-watch": "~0.5.3",
    "grunt": "~0.4.2",
    "coffee": "0.0.1",
    "chai": "~1.8.1",
    "mocha": "~1.17.1",
    "blanket": "~1.1.6"
  }
}
